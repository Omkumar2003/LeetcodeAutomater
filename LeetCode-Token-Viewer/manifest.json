{
  "manifest_version": 3,
  "name": "LeetCode Token Viewer",
  "version": "1.0",
  "description": "Displays LeetCode session and CSRF tokens.",
  "permissions": [
    "cookies"
  ],
  "host_permissions": [
    "https://leetcode.com/"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}